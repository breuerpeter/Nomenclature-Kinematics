% Encoding: UTF-8

# Position

@symbol{vecPos,
    name = {\ensuremath{\bm{r}}},
    description = {position vector},
    identifier = {position}
}

# Velocity

@symbol{vecAngVel,
    name = {\ensuremath{\bm{\omega}}},
    description = {angular velocity vector},
    identifier = {velocity}
}

@symbol{vecLinVel,
    name = {\ensuremath{\bm{v}}},
    description = {inertial linear velocity vector; for example $\gls{vecLinVel}_P\equiv{}_{\gls{frI}}\dot{\gls{vecPos}}_{\gls{frI_origin}P}$},
    identifier = {velocity}
}

# Acceleration

@symbol{vecLinAcc,
    name = {\ensuremath{\bm{a}}},
    description = {inertial linear acceleration vector; for example $\gls{vecLinAcc}_P\equiv{}_{\gls{frI}}\ddot{\gls{vecPos}}_{\gls{frI_origin}P}$},
    identifier = {visualInertialFusion}
}

# Quaternions

@symbol{quat,
    name = {\ensuremath{\mathbf{q}}},
    description = {unit quaternion},
    identifier = {quaternion}
}